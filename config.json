{
    "custom_pacman_config": true,
    "keyboard": "en",
    "wifi": {
        "state": false,
        "name": "my wifi",
        "adaptator": "wlan0"
    },
    "partitions": {
        "boot": {
            "partition": "/dev/",
            "format": true,
            "filesystem": "fat32"
        },
        "root": {
            "partition": "/dev/",
            "format": false,
            "filesystem": "ext4"
        },
        "home": {
            "partition": "/dev/",
            "format": false,
            "filesystem": "ext4"
        },
        "swap": {
            "partition": "/dev/",
            "format": false
        }
    },
    "pacstrap_skip": false,
    "additional_packages": "vim",
    "uefi": false,
    "arch-chroot": true,
    "post_install_commands": "echo Hiii!",
    "post_install_chroot_commands": "echo post install chroot command|echo YES",
    "reboot": false
}